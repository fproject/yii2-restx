{
  "name": "fproject/yii2-restx",
  "description": "Extended RESTful for Yii2 Framework",
  "keywords": ["yii2", "restx", "rest", "RESTful"],
  "homepage": "https://github.com/fproject/yii2-restx",
  "type": "yii2-extension",
  "license": "Apache-2.0",
  "support" : {
    "issues" : "https://github.com/fproject/yii2-restx/issues"
  },
  "authors": [
    {
      "name": "Bui Sy Nguyen",
      "email": "nguyenbs@f-project.net",
      "homepage": "http://f-project.net"
    }
  ],
  "minimum-stability" : "stable",
  "require" : {
    "php" : ">=5.4.0",
    "yiisoft/yii2" : "*",
    "fproject/yii2-common" : "*"
  },
  "require-dev" : {
    "codeception/codeception" : "2.0.9",
    "codeception/specify" : "*",
    "codeception/verify" : "*",
    "yiisoft/yii2-codeception" : "*"
  },
  "config" : {
    "preferred-install": "dist",
    "github-protocols": ["https","http"],
    "github-oauth": {
      "github.com": "1978ac86a7fd77ae45727ef1b39239d840c776f7"
    },
    "process-timeout" : 1800,
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      },
      "repositories": [
        {
          "type": "bower-vcs",
          "url": "https://github.com/RobinHerbots/Inputmask.git",
          "name": "bower-asset/jquery.inputmask"
        }
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "fproject\\": "fproject/"
    }
  }
}
